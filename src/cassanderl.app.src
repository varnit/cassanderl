{application, cassanderl, [
  {description, "Apache Cassandra client"},
  {vsn, "0.2"},
  {registered, []},
  {applications, [kernel, stdlib]},
  {mod, {cassanderl_app, []}},
  {env, [
    {worker_pool_size, 10},
    {hostname, "127.0.0.1"},
    {port, 9160},
    {pg2_group_name, cassanderl}
  ]}
]}.
